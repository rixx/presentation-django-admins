#db-logos img
  display: inline-block
  max-height: 200px
#migrate-code
  code, .language-text
    height: 20%
    font-size: 15px
    line-height: 1.4
  .language-shell:first-child
    border-bottom-left-radius: 0
    border-bottom-right-radius: 0
    .language-text
      padding-bottom: 1px
  .language-shell:last-child
    border-top-left-radius: 0
    border-top-right-radius: 0
    .language-text
      padding-top: 0
      code
        color: red
